<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
  assembly="Biblioseca.Model"
  namespace="Biblioseca.Model">
	<class name="Member" table="Members">
		<id name="Id">
			<generator class="identity" />
		</id>
		<property name="FirstName">
			<column name="FirstName" length="200" not-null="true" />
		</property>
		<property name="LastName">
			<column name="LastName" length="200" not-null="true" />
		</property>
		<property name="Username">
			<column name="Username" length="200" not-null="true" unique="true" />
		</property>
		<set name="Lendings" lazy="true" inverse="true" cascade="all-delete-orphan">
			<key column="MemberId"/>
			<one-to-many class="Lending"/>
		</set>
	</class>
</hibernate-mapping>
